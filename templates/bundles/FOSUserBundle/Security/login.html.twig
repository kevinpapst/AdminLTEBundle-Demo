{% extends '@AdminLTE/FOSUserBundle/Security/login.html.twig' %}

{% block logo_login %}
    <a href="{{ path('adminlte_welcome'|route_alias) }}"><b>Demo</b> Login</a>
{% endblock %}

{% block login_box_error %}
    {% if error is not defined or error is empty %}
        {% set error = {'messageKey': 'Login with "admin/password" or "user/password"', 'messageData': {}} %}
    {% endif %}
    {{ parent() }}
{% endblock %}

